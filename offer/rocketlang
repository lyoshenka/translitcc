<!-- FRONT END PROMO -->
<script type="text/javascript">$(document).ready(function(){window.loadFrontendPromo=function(lang,cb){var params={url:"www.rocketlanguages.com/sign-language/premium/?hop=axyzmedia"};if(lang){params.lang=lang;}
$.get('http://order.rocketlanguages.com/ajax/?cb=?',{call:"controllerPromo/getByPage",param:JSON.stringify(params)},function(promos){if(!promos.err&&promos.length>0&&promos[0].active){window.promo=promos[0];var inrange=false,inarea=true,debug=false,now=parseInt(+new Date()/1000),startstamp=(promo.starts)?parseInt(Date.parse(promo.starts.replace(/-/g,"/")+" UTC")/1000):false,endstamp=(promo.ends)?parseInt(Date.parse(promo.ends.replace(/-/g,"/")+" UTC")/1000):false;if(!startstamp&&!endstamp){inrange=true;}
if(now>startstamp&&now<endstamp){inrange=true;}
if(!startstamp&&now<endstamp){inrange=true;}
if(!endstamp&&now>startstamp){inrange=true;}
if(window.location.search.search("testpromo")>=0){debug=true;inrange=true;}
if(debug){console.log("Starts: (EDT) "+promo.starts);console.log("Ends: (EDT) "+promo.ends);console.log(promos);}
promo.displayPromo=function(){if(inrange){for(var j=0;j<this.sections.length;j++){var section=this.sections[j],target=(parseInt(section.insertbefore)>0)?$('<div>').insertBefore($(section.jqselector)):$(section.jqselector),secondsleft=Math.abs(endstamp-now),countdowntext="<span class='promocountdown' TTL='"+secondsleft+"'></span>",content=this.sections[j].content;if(target.length<0){if(debug)console.log("No jQuery Target")}else{content=content.replace(/\[\[countdown\]\]/ig,countdowntext);target.addClass('front-end-promo').html(content).find('.buy-now').each(function(){var newloc=section.desturl;newloc+=this.utm_campaign?"?utm_campaign="+this.utm_campaign:'';newloc+=this.utm_medium?"&utm_medium="+this.utm_medium:'';newloc+=this.utm_source?"&utm_source="+this.utm_source:'';console.log(newloc);$(this).data('href',newloc).click(function(){_gaq.push(['_trackEvent','promo',this.name,'/sign-language/premium/index.php']);console.log($(this).data('href'));window.location=$(this).data('href');return false;}).css('cursor','pointer');});(function(){var p_cd=target.find('.promocountdown'),cd_DisplayFunc=function(secs){var output='',d,h,m,s;s=secs;if(secs>=86400){d=Math.floor(s/86400);s=s%86400;output+=d+' Day'+(d!=1?'s ':' ');}
if(secs>=3600){h=Math.floor(s/3600);s=s%3600;output+=h+' Hour'+(h!=1?'s ':' ');}
if(secs>=60){m=Math.floor(s/60);s=s%60;output+=m+' Minute'+(m!=1?'s ':' ');}
